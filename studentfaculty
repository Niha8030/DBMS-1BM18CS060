create table student
(
 snum int(10),
 sname varchar2(20),
 major varchar2(20),
 lvl varchar2(3),
 age int(2),
 primary key(snum)
);

create table faculty
(
 fid int(10),
 fname varchar2(20),
 deptid int(1),
 primary key(fid)
); 

create table class
(
 cname varchar2(5),
 meets_at timestamp,
 room int(3),
 fid int(10)
 primary key(cname),
 foreign key(fid) references faculty(fid)
);

create table enrolled
(
 snum int(10),
 cname varchar2(5),
 primary key(snum,cname),
 foreign key(snum) references student(snum),
 foreign key(cname) references class(cname)
);


 
